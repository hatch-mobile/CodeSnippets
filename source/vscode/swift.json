	"Quick Comment Jira Ticket": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Jira ticket (HSD)",
		"prefix": [
			"__qchsd",
			"//qchsd",
			".qchsd"
		],
		"body": [
			"/// **Jira**",
			"///",
			"/// [HSD-${1:NNNN}](https://hatchbaby.atlassian.net/browse/HSD-${1:NNNN})",
			"///",
			"$0"
		]
	},
	"Quick Comment Local File": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Local File",
		"prefix": [
			"__qcfile",
			"//qcfile",
			".qcfile"
		],
		"body": [
			"/// **LocalFile**",
			"///",
			"/// [${1:filename}](${2:filepath})",
			"///",
			"$0"
		]
	},
	"Quick Comment See Also": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Local File",
		"prefix": [
			"__qcurl",
			"//qcurl",
			".qcurl"
		],
		"body": [
			"/// **SeeAlso**",
			"///",
			"/// [${1:name}](${2:url})",
			"///",
			"$0"
		]
	},
	"Quick Comment See Also": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Local File",
		"prefix": [
			"__qccall",
			"//qccall",
			".qccall"
		],
		"body": [
			"/// ${10:Summary}",
			"/// - Attention: Who|What",
			"/// - Authors: ${20:@user}",
			"/// - Bug: ${30:Describe}",
			"/// - Complexity: ${40:`O(n^3)`}",
			"/// - Copyright: ${50:Date}",
			"/// - Date: ${60:Date}",
			"/// - Experiment: ${70:Describe}",
			"/// - Important: ${80:Describe}",
			"/// - Invariant: ${90:Describe what doesn't change.}",
			"/// - Note: ${100:Content}",
			"/// - Parameter ${110:Name}: `${111:Type}` ${112:Describe}",
			"/// - Postcondition: ${120:Describe}",
			"/// - Precondition: ${130:Describe}",
			"/// - Remark: ${140:Describe}",
			"/// - Requires: ${150:Describe}",
			"/// - Returns: ${160:Describe}",
			"/// - Since: ${170:Introduced When}",
			"/// - Throws: ${180:Describe} `${181:Type}`",
			"/// - ToDo: ${190:Describe}",
			"/// - Version: ${200:Describe}",
			"/// - Warning: ${210:Describe}",
			"$0"
		]
	},

