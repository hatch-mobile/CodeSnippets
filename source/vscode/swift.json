{
	// Place your snippets for swift here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"// TODO: username - \\(message)": {
		"prefix": [
			"__todoc",
			"//tozc",
			".tozc"
		],
		"body": [
			"# TODO: ${10:username} - ${20:message}",
			"$0"
		]
	},
	"#warning(\"TODO: username - \\(message)\")": {
		"prefix": [
			"__todow",
			"//tozw",
			".tozw"
		],
		"body": [
			"#warning(\"FIXME: ${10:username} - ${20:message}\")",
			"$0"
		]
	},
	"// FIXME: username - \\(message)": {
		"prefix": [
			"_fixc",
			"//fizc",
			".fizc"
		],
		"body": [
			"# FIXME: ${10:username} - ${20:message}",
			"$0"
		]
	},
	"#warning(\"FIXME: username - \\(message)\")": {
		"prefix": [
			"_fixw",
			"//fizw",
			".fizw"
		],
		"body": [
			"#warning(\"FIXME: ${10:username} - ${20:message}\")",
			"$0"
		]
	},
	"Quick Comment Jira Ticket": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Jira ticket (HSD)",
		"prefix": [
			"__qchsd",
			"//qchsd",
			".qchsd"
		],
		"body": [
			"/// ## Jira",
			"///",
			"/// * [HSD-${1:NNNN}](https://hatchbaby.atlassian.net/browse/HSD-${1:NNNN})",
			"///",
			"$0"
		]
	},
	"Quick Comment Local File": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Local File",
		"prefix": [
			"__qcfile",
			"//qcfile",
			".qcfile"
		],
		"body": [
			"/// ## LocalFile",
			"///",
			"/// * [${1:filename}](${2:filepath})",
			"///",
			"$0"
		]
	},
	"Quick Comment References": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Local File",
		"prefix": [
			"__qcurl",
			"//qcurl",
			".qcurl",
			"__qcref",
			"//qcref",
			".qcref"
		],
		"body": [
			"/// ## References",
			"///",
			"/// * [${1:name}](${2:url})",
			"///",
			"$0"
		]
	},
	"Quick Comment all Callouts": {
		"scope": "c,cpp,swift",
		"description": "A quick comment for Local File",
		"prefix": [
			"__qccall",
			"//qccall",
			".qccall"
		],
		"body": [
			"/// ${10:Summary}",
			"///",
			"/// ${11:Description}",
			"///",
			"/// - Attention: Who|What",
			"/// - Authors: ${20:@user}",
			"/// - Bug: ${30:Describe}",
			"/// - Complexity: ${40:`O(n^3)`}",
			"/// - Copyright: ${50:Date}",
			"/// - Date: ${60:Date}",
			"/// - Experiment: ${70:Describe}",
			"/// - Important: ${80:Describe}",
			"/// - Invariant: ${90:Describe what doesn't change.}",
			"/// - Note: ${100:Content}",
			"/// - Parameter ${110:Name}: `${111:Type}` ${112:Describe}",
			"/// - Postcondition: ${120:Describe}",
			"/// - Precondition: ${130:Describe}",
			"/// - Remark: ${140:Describe}",
			"/// - Requires: ${150:Describe}",
			"/// - Returns: ${160:Describe}",
			"/// - Since: ${170:Introduced When}",
			"/// - Throws: ${180:Describe} `${181:Type}`",
			"/// - ToDo: ${190:Describe}",
			"/// - Version: ${200:Describe}",
			"/// - Warning: ${210:Describe}",
			"$0"
		]
	}
}