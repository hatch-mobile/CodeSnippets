	// 	],
	// 	"description": "Log output to console"
	// }

	"Comment Function": {
		"description": "A python function comment snippet",
		"prefix": [
			".comFunc"
		],
		"body": [
			"\"\"\"",
			"${1:## Summary",
			"${2:<A short description>}",
			"}",
			"${10:## Parameters",
			"* `${11:name}` (`${12:type}`): ${13:description}",
			"}",
			"${20:## Returns",
			"* (`${22:type}`): ${23:description}}",
			"\"\"\"",
			"$0"
		]
	}
}